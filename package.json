{
  "name": "advantage",
  "version": "1.0.0",
  "description": "AI-Powered Social Commerce Platform",
  "main": "index.js",
  "scripts": {
    "start": "expo start --port 8088",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:incremental": "tsc --noEmit --incremental",
    "ts:config": "tsc --showConfig",
    "ts:files": "tsc --listFilesOnly",
    "ts:trace": "tsc --traceResolution",
    "ts:explain": "tsc --explainFiles",
    "clean": "rm -rf dist .tsbuildinfo",
    "dev:check": "npm run type-check && npm run lint && npm run format:check",
    "pre-commit": "npm run type-check:incremental && npm run lint:fix && npm run format",
    "ci": "npm run type-check && npm run lint && npm run format:check"
  },
